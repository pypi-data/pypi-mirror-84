from datetime import datetime


def which_day(dt='2019-01-02'):
    dt = datetime.strptime(dt, '%Y-%m-%d')
    delta = dt - datetime(dt.year, 1, 1)
    return delta.days + 1


if __name__ == '__main__':
    print(which_day())
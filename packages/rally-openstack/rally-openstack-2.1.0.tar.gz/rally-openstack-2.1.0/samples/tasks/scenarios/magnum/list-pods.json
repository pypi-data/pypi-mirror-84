{
  "K8sPods.list_pods": [
    {
      "runner": {
        "type": "constant",
        "concurrency": 1,
        "times": 1
      },
      "context": {
        "users": {
          "users_per_tenant": 1,
          "tenants": 1
        },
        "cluster_templates": {
          "docker_volume_size": 5,
          "coe": "kubernetes",
          "image_id": "fedora-atomic-latest",
          "dns_nameserver": "8.8.8.8",
          "external_network_id": "public",
          "flavor_id": "m1.small",
          "network_driver": "flannel"
        },
        "clusters": {
          "node_count": 2
        },
        "ca_certs": {
          "directory": ""
        }
      },
      "sla": {
        "failure_rate": {
          "max": 0
        }
      }
    },
    {
      "runner": {
        "type": "constant",
        "concurrency": 1,
        "times": 1
      },
      "context": {
        "users": {
          "users_per_tenant": 1,
          "tenants": 1
        },
        "cluster_templates": {
          "docker_volume_size": 5,
          "coe": "kubernetes",
          "image_id": "fedora-atomic-latest",
          "dns_nameserver": "8.8.8.8",
          "external_network_id": "public",
          "flavor_id": "m1.small",
          "network_driver": "flannel",
          "tls_disabled": true
        },
        "clusters": {
          "node_count": 2
        }
      },
      "sla": {
        "failure_rate": {
          "max": 0
        }
      }
    }
  ]
}

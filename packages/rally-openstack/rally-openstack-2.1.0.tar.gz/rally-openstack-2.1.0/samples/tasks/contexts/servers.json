{% set flavor_name = flavor_name or "m1.tiny" %}
{
    "Dummy.openstack": [
        {
            "args": {
                "sleep": 0.1
            },
            "runner": {
                "type": "constant",
                "times": 1,
                "concurrency": 1
            },
            "context": {
                "users": {
                    "tenants": 1,
                    "users_per_tenant": 2
                },
                "servers": {
                    "flavor": {
                        "name": "{{flavor_name}}"
                    },
                    "image": {
                        "name": "^cirros.*-disk$"
                    },
                    "servers_per_tenant": 2
                }
            }
        }
    ]
}
